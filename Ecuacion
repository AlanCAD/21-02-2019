function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  var a = 3;
  var b = 15;
  var c=7;
  x = ecu (a,b,c);
   y = ecua (a,b,c);
  console.log(x,y);
}

function ecu (a,b,c) {
var x =-b+sqrt((pow(b,2)-4*a*c))/2*a;
  return ( x);
  }

function ecua(a,b,c){
   var y =-b-sqrt((pow(b,2)-4*a*c))/2*a;
  return ( y );
}

              
